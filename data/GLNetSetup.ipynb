        "def pad_image(pil_img, new_width, new_height, color):\n",
        "    width, height = pil_img.size\n",
        "    result = Image.new(pil_img.mode, (new_width, new_height), color)\n",
        "    result.paste(pil_img, (0, 0))\n",
